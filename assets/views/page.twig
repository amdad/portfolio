{% extends "tmpl/layout.twig" %}

{% block content %}
<div class="container-fluid content" data-img="{{ data.img }}" data-color="{{ data.color }}">
	<div class="row">
		<div class="col-lg-12">
    		<hgroup>
    			<h1>{{ data.Title }}</h1>
    			<h2 class="subheading">{{ data.Subtitle|raw }}</h2>
			</hgroup>
		</div>
	</div>
	{{ data.content|raw }}
</div>
<div style="height:800px;"></div>
{% endblock %}